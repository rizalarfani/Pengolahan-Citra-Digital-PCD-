    "# Menginput Angka\n",
    "angka = int(input(\"Tulis sebuah Angka: \"))\n",
    " \n",
    "#Jika Habis Dibagi Nol, Maka Genap\n",
    "if (angka % 2) == 0:\n",
    "   print(\"{0} adalah Bilangan Genap\".format(angka))\n",
    " \n",
    "#Jika Tidak Habis Dibagi Nol, Maka Ganjil\n",
    "else:\n",
    "   print(\"{0} adalah Bilangan Ganjil\".format(angka))"
